   "source": [
    "# Import modules\n",
    "from wquantiles import quantile\n",
    "\n",
    "# Function to estimate age quantiles\n",
    "def estimate_age(name, sex):\n",
    "    \n",
    "    df = get_data(name, sex)\n",
    "    dict_series = {}\n",
    "    \n",
    "    dict_series['name'] = name\n",
    "    dict_series['p_alive'] = (df['n_alive'].sum()) / (df['births'].sum())\n",
    "    dict_series['q25'] = quantile((2016 - df['year']), df['n_alive'], .25)\n",
    "    dict_series['q50'] = quantile((2016 - df['year']), df['n_alive'], .5)\n",
    "    dict_series['q75'] = quantile((2016 - df['year']), df['n_alive'], .75)\n",
    "    dict_series['sex'] = sex\n",
    "    \n",
    "    pd_series = pd.Series(dict_series)\n",
    "    \n",
    "    return pd_series\n",
    "\n",
    "# Estimate the age of a male named Bradley\n",
    "print(estimate_age('Bradley', 'M'))\n",
    "\n"
   ]
