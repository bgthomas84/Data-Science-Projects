    "# Set up matplotlib for plotting in the notebook.\n",
    "%matplotlib inline\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "sns.set()\n",
    "\n",
    "def plot_trends(name, sex):\n",
    "\n",
    "    ''' This function will plot the popularity of a name by year. '''  \n",
    "\n",
    "    name_rows = bnames.loc[(bnames['name'] == name) & (bnames['sex'] == sex), ['year', 'births']]\n",
    "    \n",
    "    x = name_rows['year']\n",
    "  \n",
    "    y = name_rows['births']\n",
    "\n",
    "    _ = plt.plot(x,y)\n",
    "    _ = plt.xlabel('Year')\n",
    "    _ = plt.ylabel('Number of Births')\n",
    "    _ = plt.title('Popularity of ' + name)\n",
    "    \n",
    "    plt.show()\n",
    "\n",
    "    return \n",
    "\n",
    "\n",
    "# Plot trends for Elizabeth and Deneen \n",
    "plot_trends('Elizabeth', 'F')\n",
    "plot_trends('Deneen', 'F')\n",
    "    \n",
    "# How many times did these female names peak?\n",
    "num_peaks_elizabeth = 3\n",
    "num_peaks_deneen    = 1"
   ]
