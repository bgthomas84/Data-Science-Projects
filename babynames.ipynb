    "def get_data(name, sex):\n",
    "    \n",
    "    df = pd.merge(bnames, lifetable_2016_s)\n",
    "    df['n_alive'] = df['births'] * df['lx'] / 100000\n",
    "    return df.loc[(df['name'] == name) & (df['sex'] == sex), :]\n",
    "    \n",
    "\n",
    "def plot_data(name, sex):\n",
    "    \n",
    "    df = get_data(name, sex)\n",
    "    ax = df.plot(x = 'year', y = 'births', label = 'births')\n",
    "    _ = df.plot(x = 'year', y = 'n_alive', kind = 'area', label = 'n_alive', ax = ax)\n",
    "    plt.show()\n",
    "    \n",
    "    return \n",
    "    \n",
    "# Plot the distribution of births and number alive for Joseph and Brittany\n",
    "plot_data('Joseph', 'M')\n",
    "plot_data('Brittany', 'F')\n"
